currentvalue: number;\n\n\n  ngOnInit() {\n\n    if ( Number(this.codevalue) - 1000 < 0 ) {\n      this.rangemin = '0';\n    }\n\n  }\n\n  btnRup() {\n    this.rangeMM = this.rangeMM + 10 ;\n\n    this.rangemax = String(Number(this.codevalue) + this.rangeMM);\n    this.rangemin = String(Number(this.codevalue) - this.rangeMM);\n\n  }\n\n  btnRdown() {\n    if (this.rangeMM > 0) {\n      this.rangeMM = this.rangeMM - 10 ;\n      this.rangemax = String(Number(this.codevalue) + this.rangeMM);\n      this.rangemin = String(Number(this.codevalue) - this.rangeMM);\n    }\n\n  }\n\n  btnSup() {\n    this.step = this.step + 1 ;\n  }\n\n  btnSdown() {\n    if (this.step > 0) {\n      this.step = this.step - 1 ;\n    }\n  }\n\n\n\n}\n"],"sourceRoot":"webpack:///"}